@{
    Layout = null;
    ViewData["Title"] = "You Have Submitted Following Info";
}
<div>
    <div class="taskItem border-bottom d-flex">
        <div class="d-flex flex-row m-4 w-100">
            <div class="CheckBox-Block">
                <fieldset class="form-group">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="true" id="flexCheckDefault">
                        <label class="form-check-label" for="flexCheckDefault">
                        </label>
                    </div>
                </fieldset>
            </div>
            <button class="taskItem-titleWrapper ">
                <span class="taskItem-title text-left">@ViewBag.TaskTitle</span>
                <div class="metaDataInfo "></div>
            </button>
        </div>
    </div>
</div>